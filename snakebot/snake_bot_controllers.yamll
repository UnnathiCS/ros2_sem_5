controller_manager:
  ros__parameters:
    update_rate: 100

    joint_state_broadcaster:
      type: joint_state_broadcaster/JointStateBroadcaster

    # Individual joint controllers for each joint
    snake_body_1_aux_joint_controller:
      type: effort_controllers/JointPositionController
      joint: snake_body_1_aux_joint
      gains:
        p: 50.0
        i: 0.0
        d: 1.0
    snake_body_1_joint_controller:
